{
	"compilerOptions": {
		"target": "ES2021",
		"module": "Preserve",
		"moduleResolution": "Bundler",
		"jsx": "react-jsx",
		"strict": true,
		"noEmit": true,
		"lib": ["ES2021", "DOM"],
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"noUnusedLocals": true
	}
}
